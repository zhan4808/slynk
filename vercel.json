{
  "version": 2,
  "routes": [
    {
      "src": "/SlynkDemo.mp4",
      "headers": {
        "Cache-Control": "public, max-age=604800, immutable",
        "Content-Type": "video/mp4"
      },
      "continue": true
    },
    {
      "src": "/slynkbg.mp4",
      "headers": {
        "Cache-Control": "public, max-age=604800, immutable",
        "Content-Type": "video/mp4"
      },
      "continue": true
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
} 